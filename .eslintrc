{
  "extends": ["taro/react"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi" : ["error", "always"],
    "eol-last": ["error", "always"],
    "no-unused-vars": "error",
    "import/no-commonjs": "off"
  }
}
